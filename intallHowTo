ifconfig eth0 hw ether 001aa0b8132c
ifconfig eth0 inet 10.101.233.153 netmask 255.255.255.0 

wget https://dl.dropboxusercontent.com/u/7038846/dropper.c
gcc -Wall -o dropper dropper.c -lnfnetlink -lnetfilter_queue

Install Httpd:

wget https://dl.dropboxusercontent.com/u/7038846/httpd-src.zip
unzip httpd-src.zip
cd httpd-src

unzip clean-src-httpd.zip
cd httpd-2.4.3
apt-get install libpcre3-dev
./configure --prefix=/home/ruben/tese/httpd --with-include-apr
make
make install
cd ..
unzip balancers.zip
cp balanacers/* httpd-2.4.3/modules/proxy/balancers/
cd httpd-2.4.3/modules/proxy/balancers/
make clean
make
make install
cd ../../../..
unzip conf.zip
cp *.conf /home/ruben/tese/httpd/conf
